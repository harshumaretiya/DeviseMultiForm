Social
<%binding.pry%>
<%= form_for('', url: social_user_step_path, method: :post) do |f| %>
  <h2>Tell us a little about yourself</h2>
     <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", required: true,value: session[:email] %>
  </div>
  <div class="field">
  <%= f.label :city %>
  <%= f.text_field :city, value: session[:first_name] %>
</div>
<div class="field">
  <%= f.label :state %>
  <%= f.text_field :state %>
</div>
<div class="field">
  <%= f.label :country %>
  <%= f.text_field :country %>
</div>

<div class="field">
  <%= f.label :website %>
  <%= f.url_field :website %>
</div>
<div class="field">
  <%= f.label :skills %>
  <%= f.text_field :skills %>
</div>

<div class="field">
  <%= f.label :passion %>
  <%= f.text_field :passion %>
</div>

<div class="field">
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>
  <div class="actions">
        <%= f.submit "Continue" %>
  </div>
<% end %>